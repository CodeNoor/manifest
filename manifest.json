{
    "name": "Searchbar",
    "version": "1.0",
    "description": "Simple searchbar",
    "permissions": ["storage"],
    "chrome_url_overrides": {
      "newtab": "popup.html"
    },
    "manifest_version": 2
  ,
  "content_scripts": [
    {
      "matches": [ "<all_urls>"],
      "js": ["content.js"]
    }
  ]}